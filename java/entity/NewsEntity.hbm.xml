<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.NewsEntity" table="news" schema="ssh">
        <id name="id" column="id"/>
        <property name="newstitle" column="newstitle"/>
        <property name="source" column="source"/>
        <property name="context" column="context"/>
        <property name="addtime" column="addtime"/>
        <property name="photo" column="photo"/>
        <property name="newsauthor" column="newsauthor"/>
    </class>
</hibernate-mapping>