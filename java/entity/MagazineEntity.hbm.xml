<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.MagazineEntity" table="magazine" schema="ssh">
        <id name="id" column="id"/>
        <property name="magatitle" column="magatitle"/>
        <property name="magawriter" column="magawriter"/>
        <property name="book" column="book"/>
        <property name="addtime" column="addtime"/>
    </class>
</hibernate-mapping>